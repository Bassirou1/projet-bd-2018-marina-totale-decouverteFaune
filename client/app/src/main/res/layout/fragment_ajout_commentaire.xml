<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <TextView
        android:id="@+id/fragment_ajout_commentaire_text_titre"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_marginStart="27dp"
        android:layout_marginTop="17dp"
        android:text="Ajouter un commentaire"
        android:textSize="24sp" />

    <TextView
        android:id="@+id/fragment_ajouter_sous_titre"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_marginStart="21dp"
        android:layout_marginTop="79dp"
        android:text="Ajoutez un commentaire et une note"
        android:textSize="14sp" />

    <EditText
        android:id="@+id/fragment_ajout_text_commentaire"
        android:layout_width="match_parent"
        android:layout_height="137dp"
        android:layout_alignParentStart="true"
        android:layout_marginStart="0dp"
        android:layout_marginTop="300dp"
        android:layout_marginBottom="100dp"
        android:gravity="top"
        android:inputType="text|textMultiLine" />

    <TextView
        android:id="@+id/textView10"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true"
        android:layout_alignParentLeft="true"
        android:layout_alignParentTop="true"
        android:layout_marginStart="17dp"
        android:layout_marginLeft="17dp"
        android:layout_marginTop="254dp"
        android:text="Commentaire : " />

    <Button
        android:id="@+id/action_fragment_navigation_detail"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentEnd="true"
        android:layout_alignParentRight="true"
        android:layout_alignParentBottom="true"
        android:layout_marginEnd="68dp"
        android:layout_marginRight="68dp"
        android:layout_marginBottom="49dp"
        android:text="Ajouter" />

    <Button
        android:id="@+id/fragment_ajout_commentaire_1_etoile"
        android:layout_width="68dp"
        android:layout_height="68dp"
        android:layout_alignParentStart="true"
        android:layout_alignParentLeft="true"
        android:layout_alignParentTop="true"
        android:layout_marginStart="22dp"
        android:layout_marginLeft="22dp"
        android:layout_marginTop="133dp"
        android:background="@android:color/transparent"
        android:text=""
        android:visibility="visible" />

    <TextView
        android:id="@+id/textView11"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignStart="@+id/fragment_ajouter_sous_titre"
        android:layout_alignParentTop="true"
        android:layout_marginTop="107dp"
        android:text="Vote : " />

    <Button
        android:id="@+id/fragment_ajout_commentaire_4_etoile"
        android:layout_width="68dp"
        android:layout_height="68dp"
        android:layout_alignTop="@+id/textView11"
        android:layout_alignEnd="@+id/fragment_ajout_commentaire_image_etoile"
        android:layout_marginTop="25dp"
        android:layout_marginEnd="89dp"
        android:background="@android:color/transparent"
        android:text=""
        android:visibility="visible" />

    <Button
        android:id="@+id/fragment_ajout_commentaire_2_etoile"
        android:layout_width="68dp"
        android:layout_height="68dp"
        android:layout_alignParentStart="true"
        android:layout_alignParentLeft="true"
        android:layout_alignParentTop="true"
        android:layout_marginStart="90dp"
        android:layout_marginLeft="90dp"
        android:layout_marginTop="133dp"
        android:background="@android:color/transparent"
        android:text=""
        android:visibility="visible" />

    <Button
        android:id="@+id/fragment_ajout_commentaire_3_etoile"
        android:layout_width="68dp"
        android:layout_height="68dp"
        android:layout_alignTop="@+id/fragment_ajouter_sous_titre"
        android:layout_marginStart="69dp"
        android:layout_marginTop="54dp"
        android:layout_toEndOf="@+id/fragment_ajout_commentaire_1_etoile"
        android:background="@android:color/transparent"
        android:text=""
        android:visibility="visible" />

    <Button
        android:id="@+id/fragment_ajout_commentaire_5_etoile"
        android:layout_width="68dp"
        android:layout_height="68dp"
        android:layout_below="@+id/fragment_ajouter_sous_titre"
        android:layout_alignEnd="@+id/fragment_ajout_commentaire_3_etoile"
        android:layout_marginTop="34dp"
        android:layout_marginEnd="-136dp"
        android:background="@android:color/transparent"
        android:text=""
        android:visibility="visible" />

    <ImageView
        android:id="@+id/fragment_ajout_commentaire_image_etoile"
        android:layout_width="match_parent"
        android:layout_height="73dp"
        android:layout_below="@+id/fragment_ajouter_sous_titre"
        android:layout_alignEnd="@+id/fragment_ajouter_sous_titre"
        android:layout_marginTop="31dp"
        android:layout_marginEnd="-138dp"
        app:srcCompat="@drawable/starrating0of5" />


</RelativeLayout>